{
    "headers": [
        {
            "source": "/api/save_form_data",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://chefs-bhojan-form.vercel.app"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "POST, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Credentials",
                    "value": true
                }
            ]
        },
        {
            "source": "/api/get_discount_value",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "*"
                }
            ]
        }
    ],
    "version": 2,
    "builds": [
        {
            "src": "index.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.12"
            }
        }
    ]
}